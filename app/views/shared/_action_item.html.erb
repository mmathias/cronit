<li id="<%= action_item.id %>">
  <span class="content"><%= action_item.id %> </span>
  How Long:
  <span class="content"><%= action_item.how_long %> </span>
  </span>
  <% if current_user?(action_item.user) %>
    <%= link_to "delete", action_item, method: :delete,data: {confirm: "You sure?"}, title: action_item.id %>
  <% end %>
</li>